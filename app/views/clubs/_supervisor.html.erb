<div class="container">
  <div class="row">
    <div class="col-md-9">
      <div class="col-lg-9">
          <h6 class="description">Denetim Kurulu</h6>
          <div class="row pt-md">
            <% if @club_view_board_of_supervisor %>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.principal_member_first, nil, 150, :class => "img-responsive") %>
              </div>
              <h1><%=profile_or_not?(@club_period.club_board_of_supervisory.principal_member_first, "full_name", "full_name") %></h1>

              <h2>Asıl Üye</h2>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.principal_member_second, nil, 150, :class => "img-responsive") %>
              </div>
              <h1><%=profile_or_not?(@club_period.club_board_of_supervisory.principal_member_second, "full_name", "full_name") %></h1>
              <h2>Asıl Üye</h2>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.principal_member_third, nil, 150, :class => "img-responsive") %>
              </div>
                  <h1><%=profile_or_not?( @club_period.club_board_of_supervisory.principal_member_third, "full_name", "full_name") %></h1>
              <h2>Asıl Üye</h2>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.reserve_member_first, nil, 150, :class => "img-responsive") %>
              </div>
                <h1><%=profile_or_not?( @club_period.club_board_of_supervisory.reserve_member_first, "full_name", "full_name") %></h1>
              <h2>Yedek Üye</h2>
