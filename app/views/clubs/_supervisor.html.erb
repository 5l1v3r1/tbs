            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.reserve_member_second, nil, 150, :class => "img-responsive") %>
              </div>
                <h1><%=profile_or_not?( @club_period.club_board_of_supervisory.reserve_member_second, "full_name", "full_name") %></h1>
              <h2>Yedek Üye</h2>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12 profile">
              <div class="img-box">
                <%= avatar_or_not?(@club_period.club_board_of_supervisory.reserve_member_third, nil, 150, :class => "img-responsive") %>
              </div>
              <h1><%=profile_or_not?(  @club_period.club_board_of_supervisory.reserve_member_third, "full_name", "full_name") %></h1>
              <h2>Yedek Üye</h2>
            </div>
            <% end %>
          </div>
          <% if current_user.present? && current_user.admin? %>
          <%= link_to "Denetim Kurulu Düzenle", @club_period.club_board_of_supervisory.present? ? edit_club_board_of_supervisory_path(@club_period.club_board_of_supervisory)  : new_club_board_of_supervisory_path %>
         <%end%>
      </div>
    </div>
  </div>
</div>
