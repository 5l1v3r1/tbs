<%= simple_form_for(@club_setting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :club_id, collection: Club.all, :label_method => :name, label: 'Topluluk Seçiniz.' %>
    <%= f.input :max_user, label: "Maksimum Üye Sayısı" %>
    <%= f.input :is_active, label: "Aktif" %>
    <%= f.input :program_id, label: "Program kodları (Mesleki Topluluklar için.)" %>
  </div>

  <div class="form-actions">
    <%= f.submit "Kaydet", class: "btn btn-success" %>
  </div>
<% end %>
