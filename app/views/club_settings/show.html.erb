<div class="row">
  <%= render 'home/navbarDefault' %>
</div>
<div class="row">
  <div class="col-md-12">
    <ol class="breadcrumb">
      <li><%= link_to 'Anasayfa', root_path %></li>
      <li><%= link_to 'Topluluk Ayarıları', club_settings_path %></li>
      <li class="active">Topluluk Ayarı Detay</li>
    </ol>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Topluluk Ayarı Detay</h3>
      </div>
      <div class="panel-body">
        <p>
          <strong>Topluluk:</strong>
          <%= @club_setting.club.name %>
        </p>
        <p>
          <strong>Maksimum Kullanıcı Sayısı:</strong>
          <%= @club_setting.max_user %>
        </p>
        <p>
          <strong>Aktif/Pasif:</strong>
          <%= @club_setting.is_active ? "Aktif" : "Pasif" %>
        </p>
        <p>
          <strong>Program kodları (Mesleki Topluluklar için.):</strong>
          <%= @club_setting.program_id %>
        </p>
        <%= link_to 'Düzenle', edit_club_setting_path(@club_setting), class: "btn btn-success" %> |
        <%= link_to 'Geri', :back, class: "btn btn-info" %>
      </div>
    </div>
  </div>
</div>
