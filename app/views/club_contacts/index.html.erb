<div class="row">
  <%= render 'home/navbarDefault' %>
</div>
<div class="row">
  <div class="col-md-12">
  <ol class="breadcrumb">
    <li><%= link_to 'Anasayfa', root_path %></li>
    <li class="active">Topluluk İletişim Bilgileri</li>
  </ol>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Topluluk İletişim Bilgileri</h3>
      </div>
      <div class="panel-body">
      <p align="right">
      <%= link_to 'Yeni Topluluk İletişim Bilgileri', new_club_contact_path, class: "btn btn-default" %>
      </p>
<table class="table table-hover" id="club_contact_table">
  <thead>
    <tr>
      <th></th>
      <th>Topluluk</th>
      <th>Email</th>
      <th>Telefon Numarası</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @club_contacts.each do |club_contact| %>
      <tr>
        <td></td>
        <td><%= link_to club_contact.club.name, club_contact %></td>
        <td><%= club_contact.email %></td>
        <td><%= club_contact.phone_number %></td>
        <td><%= link_to '<i class="fa fa-trash" title="Sil"></i>'.html_safe, club_contact, method: :delete, data: { confirm: 'Topluluk iletişim bilgilerini silmek istediğinize emin misiniz?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

      <br>
      </div>
    </div>
  </div>
</div>